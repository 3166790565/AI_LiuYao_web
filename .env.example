# 应用配置
SECRET_KEY=your_secret_key_here

# 数据库配置
# SQLite配置（默认）
SQLALCHEMY_DATABASE_URI=sqlite:///app.db
# MySQL配置（可选）
# SQLALCHEMY_DATABASE_URI=mysql://username:password@localhost/dbname

# SQLAlchemy配置
SQLALCHEMY_TRACK_MODIFICATIONS=False

# 日志配置
LOG_LEVEL=INFO

# 文件存储配置
HISTORY_DIR=history

# 支持的模型配置
SUPPORTED_MODELS=gpt-4,gpt-4.1
